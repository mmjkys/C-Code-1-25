#include<iostream>
using namespace std;

int main() {
	for (int i = 0; i < 1; i++) {
		for (int j = 0; j < 5; j++) {
			cout << "3";
		}
		cout << endl;
	}
	for (int i = 0; i < 1; i++) {
		for (int j = 0; j < 5; j++) {
			cout << "4";
		}
		cout << endl;
	}
	for (int i = 0; i < 1; i++) {
		for (int j = 0; j < 5; j++) {
			cout << "5";
		}
		cout << endl;
	}

	for (int i = 0; i < 6; i++) {
		for (int j = 0; j < 10; j++) {
			cout << "$";
		}
		cout << endl;
	}

}//end of main
